#/a.team.mtp.rc

# Define Service
service a.team.mtp /system/bin/A-Team-MTP.sh

# Don't Start Unless Asked To
       disabled

# Only Execute Once
#       oneshot

# run unrestricted SELinux context
       seclabel u:r:magisk:s0

# Start Service When Boot Complete
on property:sys.boot_completed=1
       start a.team.mtp
